<div class='contact_counter'>There are <span class='counter_number'><%= Contact.all.length %></span> contacts</div>

<div class='contact'>
  <% @contacts.each do |contact| %>
    <ul>
      <li>
        <h3><a href='/contacts/<%= contact.id %>'><%= contact.full_name %></a></h3>
      </li>
    </ul>
  <% end %>
</div>
